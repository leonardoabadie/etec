algoritmo "exemplo_algoritmo"

var

cont, soma, n: inteiro

inicio

repita
    escreva("Digite um número inteiro: ")
    leia(n)
    
    soma <- soma + n
    cont <- cont + 1

ate (cont = 5)

escreval("O valor do contador é: ", cont)
escreval("O valor da soma é: ", soma)

fimalgoritmo
